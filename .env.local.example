# LeadFlow Pro Environment Variables
# Copy this to .env.local and fill in your values

# ============================================
# SUPABASE (Required for Analytics & Database)
# ============================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# ============================================
# TWILIO (Required for Voice Agent)
# ============================================
# Get credentials from: https://console.twilio.com
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_PHONE_NUMBER=+19522951346

# Webhook URL for Twilio (update with your domain)
TWILIO_WEBHOOK_URL=https://your-domain.com/api/voice/webhook

# ============================================
# ELEVENLABS (Required for Text-to-Speech)
# ============================================
# Get API key from: https://elevenlabs.io
ELEVENLABS_API_KEY=sk_xxxxxxxxxxxxxxxxxxxxxxxxxxxxx
ELEVENLABS_VOICE_ID=EXAVITQu4vr4xnSDxMaL  # Sarah - Swiss German Female

# ============================================
# GOOGLE PLACES API (Optional - for Lead Finding)
# ============================================
NEXT_PUBLIC_GOOGLE_PLACES_API_KEY=your-google-places-api-key

# ============================================
# EMAIL (Resend - Optional)
# ============================================
RESEND_API_KEY=re_xxxxxxxxxxxxxxxxxxxxx

# ============================================
# APP CONFIG
# ============================================
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=LeadFlow Pro
